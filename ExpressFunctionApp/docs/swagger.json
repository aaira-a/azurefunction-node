{
  "swagger": "2.0",
  "info": {
    "version": "0.0.1",
    "title": "Az Function Mock Web Service",
    "description": "Azure Function Mock Web Service"
  },
  "host": "azamwebsvc.azurewebsites.net",
  "basePath": "/api",
  "schemes": [
    "https"
  ],
  "consumes": [
    "application/json"
  ],
  "produces": [
    "application/json"
  ],
  "paths": {
    "/docs/swagger.json": {
      "get": {
        "summary": "Get this Swagger 2.0 definition",
        "description": "Get this Swagger 2.0 definition",
        "responses": {
          "200": {
            "description": "OK",
            "schema": {
              "type": "file"
            }
          }
        }
      }
    },
    "/echo/{status}": {
      "post": {
        "summary": "Echoes back request",
        "description": "Echoes headers, querystring, and request body",
        "parameters": [
          {
            "in": "path",
            "name": "status",
            "description": "Status code to be returned",
            "required": true,
            "type": "integer"
          },
          {
            "in": "body",
            "name": "echoRequest",
            "description": "Request body to be echoed",
            "schema": {
              "$ref": "#/definitions/echoRequest"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK",
            "schema": {
              "$ref": "#/definitions/echoResponse"
            }
          }
        }
      }
    },
    "/files/errors/{status}": {
      "get": {
        "summary": "Download error",
        "description": "Returns error code instead of image for download",
        "parameters": [
          {
            "in": "path",
            "name": "status",
            "description": "Error status code to be returned",
            "required": true,
            "type": "integer"
          }
        ],
        "produces": [
          "images/jpeg"
        ],
        "responses": {
          "200": {
            "description": "OK",
            "schema": {
              "type": "object",
              "properties": {
                "file": {
                  "type": "string",
                  "format": "byte"
                }
              }
            }
          }
        }
      }
    },
    "/all-types": {
      "post": {
        "summary": "POST all variable types",
        "description": "POST all variable types",
        "parameters": [
          {
            "in": "body",
            "name": "allTypesInputs",
            "required": true,
            "schema": {
              "$ref": "#/definitions/allTypesInputs"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK",
            "schema": {
              "$ref": "#/definitions/allTypesOutputs"
            }
          }
        }
      }
    }
  },
  "definitions": {
    "echoRequest": {
      "type": "object"
    },
    "echoResponse": {
      "type": "object"
    },
    "allTypesInputs": {
      "type": "object",
      "properties": {
        "textInput": {
          "type": "string",
          "description": "Text Input"
        },
        "decimalInput": {
          "type": "number",
          "description": "Decimal Input"
        },
        "integerInput": {
          "type": "integer",
          "description": "Integer Input"
        },
        "booleanInput": {
          "type": "boolean",
          "description": "Boolean Input"
        },
        "datetimeInput": {
          "type": "string",
          "format": "date-time",
          "description": "Datetime Input"
        },
        "collectionInput": {
          "type": "array",
          "items": {
            "type": "string",
            "description": "Collection Item Input"
          },
          "description": "Collection Input"
        }
      }
    },
    "allTypesOutputs": {
      "type": "object",
      "properties": {
        "textOutput": {
          "type": "string",
          "description": "Text Output"
        },
        "decimalOutput": {
          "type": "number",
          "description": "Decimal Output"
        },
        "integerOutput": {
          "type": "integer",
          "description": "Integer Output"
        },
        "booleanOutput": {
          "type": "boolean",
          "description": "Boolean Output"
        },
        "datetimeOutput": {
          "type": "string",
          "format": "date-time",
          "description": "Datetime Output"
        },
        "collectionOutput": {
          "type": "array",
          "items": {
            "type": "string",
            "description": "Collection Item Output"
          },
          "description": "Collection Output"
        }
      }
    }
  }
}
